{
    "name": "Extension",
    "version": "1.0",
	"description": "Build an Extension!",
	"permissions": ["activeTab", "declarativeContent", "storage","http://localhost/*"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
	},
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	  },
    "manifest_version": 2
  }